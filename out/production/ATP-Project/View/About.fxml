<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="430.0" prefWidth="600.0" stylesheets="@ProjectStyle.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.About">
    <children>
        <Label fx:id="text" alignment="TOP_LEFT" layoutX="8.0" layoutY="8.0" prefHeight="500.0" prefWidth="500.0" />
        <Button fx:id="exit" layoutX="200.0" layoutY="156.0" mnemonicParsing="false" onAction="#closePane" text="Close Window" />
        <ImageView fx:id="exitTop" fitHeight="80.0" fitWidth="80.0" layoutX="100.0" layoutY="130.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</Pane>
